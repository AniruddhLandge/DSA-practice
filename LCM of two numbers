#include<iostream>
using namespace std;
int main(){
    int a = 20, b = 28;
    int gcd = 1, lcm=1;
    for(int i=1;i<min(a,b);i++){
        if(a%i==0 && b%i==0){
            gcd = i;
        }
    }
    lcm = (a*b)/gcd;  //formula
    cout<<"the LCM of "<<a<<" and "<<b<<" is: "<<lcm;
    return 0;
}
