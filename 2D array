#include<iostream>
using namespace std;
int main(){
    int arr[4][3];
    //arr[2][1] = 18;   //replace element with 18
    // cout<<arr[2][1];
      int rows = 4;
      int cols = 3;
      cout<<"Enter the numbers: ";
    for(int i=0; i<rows; i++){
       for(int j=0; j<cols; j++){
        cin>>arr[i][j];
       }
    }
    for(int i=0; i<rows; i++){
       for(int j=0; j<cols; j++){
        cout<<arr[i][j]<<" ";
       }
       cout<<endl;
    }
    return 0;
}
